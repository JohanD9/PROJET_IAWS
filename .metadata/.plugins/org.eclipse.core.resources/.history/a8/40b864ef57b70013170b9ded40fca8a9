package gestionST4;

public class Position {
	private long lat;
	private long lng;

	public long getLat() {
		return lat;
	}

	public void setLat(long lat) {
		this.lat = lat;
	}

	public long getLng() {
		return lng;
	}

	public void setLng(long lng) {
		this.lng = lng;
	}

	@Override
	public String toString() {
		return "Position (lat : " + lat + ", lng : " + lng + ")";
	}
	
	
	
	
}
